; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Ad-Racing"
run/main_scene="res://MainCurve.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
boot_splash/image="res://Art/Cars/cyan.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"
boot_splash/minimum_display_time=5

[audio]

buses/default_bus_layout="res://buses.tres"

[display]

window/size/viewport_width=1280
window/size/viewport_height=760
window/stretch/mode="viewport"
window/stretch/aspect="ignore"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
occlusion_culling/use_occlusion_culling=true
2d/snap/snap_2d_transforms_to_pixel=true
anti_aliasing/main_scene="res://MainCurve.tscn"
